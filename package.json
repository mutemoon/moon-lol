{
  "name": "fsm-demo-rust",
  "scripts": {
    "extract": "cargo run --example extract",
    "hot:patch": "dx serve --hot-patch --features \"bevy/hotpatching\"",
    "test:minion:watch": "cargo-watch -x \"test -p moon_lol --test minion -- tests::test_complete_attack_cycle --exact --nocapture\"",
    "example:stream:watch": "cargo-watch -x \"run --example stream\"",
    "example:attack:watch": "cargo-watch -x \"run --example attack\" -w src -w assets -w crates",
    "example:attack": "cargo run --package moon_lol --example attack",
    "example:attack:release": "cargo run --package moon_lol --example attack --release",
    "example:classic_fiora": "cargo run --package moon_lol --example classic_fiora",
    "example:classic_fiora:watch": "cargo-watch -w src -w crates -w examples -w assets -x \"run --example classic_fiora\"",
    "example:classic_fiora:release": "cargo run --package moon_lol --example classic_fiora --release",
    "example:navigation_grid": "cargo run --package moon_lol --example navigation_grid",
    "example:particle:watch": "cargo-watch -w src -w crates -w examples -w assets/shaders -x \"run --example particle\"",
    "example:texture:watch": "cargo-watch -w src -w crates -w examples -w assets/shaders -x \"run --example texture\""
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.0",
    "@gltf-transform/extensions": "^4.2.0",
    "@types/node": "^24.0.14",
    "three": "^0.178.0",
    "vite": "^6.0.6"
  },
  "devDependencies": {
    "@types/three": "^0.178.1"
  }
}
