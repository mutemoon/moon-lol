{
  "name": "fsm-demo-rust",
  "scripts": {
    "extract": "cargo run --example extract",
    "example:attack:watch": "cargo-watch -x \"run --example attack\" -w src -w assets -w crates",
    "example:attack": "cargo run --package moon_lol --example attack",
    "example:classic_fiora": "cargo run --package moon_lol --example classic_fiora",
    "example:navigation_grid": "cargo run --package moon_lol --example navigation_grid",
    "example:particle:watch": "cargo-watch -w src -w crates -w examples -w assets/shaders -x \"run --example particle\"",
    "example:texture:watch": "cargo-watch -w src -w crates -w examples -w assets/shaders -x \"run --example texture\""
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.0",
    "@gltf-transform/extensions": "^4.2.0",
    "@types/node": "^24.0.14",
    "three": "^0.178.0",
    "vite": "^6.0.6"
  },
  "devDependencies": {
    "@types/three": "^0.178.1"
  }
}